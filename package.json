  "repository": {
    "type": "git",
    "url": "https://github.com/dongcai/react-redux-semantic-ui"
  },
  "homepage": "https://github.com/dongcai/react-redux-semantic-ui",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel",
    "semantic-ui"
  ],
  "main": "bin/server.js",
  "jest": {
    "verbose": true
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "cross-env NODE_PATH=src NODE_ENV=production PORT=8080 APIPORT=3030 node ./bin/server.js",
    "start-prod-api": "cross-env NODE_PATH=api NODE_ENV=production APIPORT=3030 node ./bin/api.js",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "start-dev": "cross-env NODE_PATH=src NODE_ENV=development PORT=3000 APIPORT=3030 WEBPACK_DLLS=1 node ./bin/server.js",
    "start-dev-api": "cross-env NODE_PATH=api NODE_ENV=development APIPORT=3030 node ./bin/api.js",
    "watch-client": "cross-env UV_THREADPOOL_SIZE=100 NODE_PATH=src NODE_ENV=development PORT=3000 APIPORT=3030 WEBPACK_DLLS=1 node webpack/webpack-dev-server.js",
    "build": "cross-env NODE_ENV=production webpack --colors --display-error-details --config=webpack/prod.config.js",
    "build-dlls": "webpack --colors --display-error-details --config webpack/vendor.config.js",
    "postinstall": "concurrently \"npm run build\" \"npm run build-dlls\"",
    "prettier": "prettier-eslint --write --eslint-config-path .eslintrc './+(src|api)/**/*.js'",
    "lint": "eslint -c .eslintrc api src",
    "test": "cross-env NODE_PATH=src NODE_ENV=test jest src",
    "test-node": "cross-env NODE_PATH=api jest api --verbose --debug",
    "lighthouse-report": "lighthouse http://localhost:8080 --view",
    "docker-build": ". ./docker/build.sh",
    "docker-dev": ". ./docker/run.dev.sh",
    "docker-prod": ". ./docker/run.prod.sh"
  },
  "lint-staged": {
    "+(src|api)/**/*.js": [
      "prettier-eslint --write --eslint-config-path .eslintrc",
      "eslint -c .eslintrc",
      "git add"
    ]
  },
